{
    "DATASET_CONFIG": {
        "data_root": "data/",
        "string_pad": 30,
        "ttv_split": [0.8, 0.2]
    },

    "TRAIN_CONFIG": {
        "lr": 3e-4,
        "n_epochs": 150,
        "batch_size": 32, 
        "A_name": "lstm",
        "embedding_type": "one_hot",
        "n_tokens": 30
    },

    "MODEL_KWARGS": {
        "input_size": 26,
        "hidden_size": 50,
        "num_layers": 1,
        "bias": true,
        "batch_first": true,
        "dropout": 0,
        "bidirectional": false,
        "proj_size": 0
    }
}